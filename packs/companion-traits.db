{"name":"Light Plating","type":"feat","img":"modules/sw5e-companions/icons/droid.webp","data":{"description":{"value":"<p>Your companion gains proficiency in light armor. If your companion is already proficient in light armor, instead your companion’s speed increases by 5 feet while light armor is integrated.</p>","chat":"","unidentified":""},"requirements":"Droid","source":"Expanded Content","activation":{"type":"","cost":null,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["2d6","ion"]],"versatile":""},"formula":"","save":{"ability":"dex","dc":null,"scaling":"int"},"recharge":{"value":null,"charged":false},"cover":null},"effects":[{"_id":"nsTYMkpwjPjiy6dE","changes":[{"key":"data.traits.armorProf.value","mode":2,"value":"lgt","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":"icons/svg/aura.svg","label":"Light Plating","origin":"Item.p7BIolD7EyJn9aXD","transfer":true,"flags":{"dae":{"selfTarget":false,"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true},"core":{"statusId":""}},"tint":null,"selectedKey":"data.traits.armorProf.value"},{"_id":"4dUry782zndlwyMA","changes":[{"key":"data.attributes.movement.walk","mode":2,"value":"5","priority":"20"}],"disabled":true,"duration":{"startTime":null},"icon":"icons/svg/aura.svg","label":"Light Plating (+5 ft)","origin":"Item.p7BIolD7EyJn9aXD","transfer":true,"flags":{"dae":{"selfTarget":false,"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true},"core":{"statusId":""}},"tint":null,"selectedKey":"data.attributes.movement.walk"}],"folder":null,"sort":0,"permission":{"default":0,"l4V5sWdK1q3eXkPg":3},"flags":{"core":{"sourceId":"Item.r4Fy